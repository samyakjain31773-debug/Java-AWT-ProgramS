import java.awt.*;
public class Textfield {

    
    public static void main(String[]args){

        Frame f = new Frame("Form");
        Label l = new Label ("Name:");

        TextField t = new TextField();

        l.setBounds(50,80,50,30);
        t.setBounds(120,80,150,30);

        f.add(l);
        f.add(t);

        f.setSize(300,250);
        f.setLayout(null);
        f.setVisible(true);
    }
}
